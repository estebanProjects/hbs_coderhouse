<div>
    <h1>Vista de productos</h1>
    <div class="main-container">
        <table>
            <thead>
                <tr>
                    <th>Nombre</th><th>Precio</th><th>Foto</th>
                </tr>
            </thead>

            <ul>
                {{#each data}}
                <tr>
                    <td>
                        {{this.title}}
                    </td>
                    <td>
                        {{this.price}}
                    </td>
                    <td>
                        <img src="
                        {{this.thumbnail}}
                        " alt="">
                    </td>
                </tr>
                {{/each}}
            </ul>
        </table>
        <p>
            <a href="/">Volver al formulario</a>
        </p>
    </div>
</div>